{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-named-as-default-member": [0]
  },
  "globals": {
    "document": true,
    "window": true,
    "fetch": true,
    "navigator": true,
    "describe": true,
    "it": true,
    "expect": true
  }
}
